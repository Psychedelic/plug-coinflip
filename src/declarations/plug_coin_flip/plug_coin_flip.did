type Profile_2 = record {
    "principal": text;
    "score": nat32;
    "attempts": nat32;
    "ratio": nat32;
};

type Profile = Profile_2;

service : {
    "getSelf": () -> (Profile) query;
    "get": (text) -> (Profile) query;
    "leaderboard": () -> (vec Profile) query;
    "coinFlip": (bool) -> (bool);
}
